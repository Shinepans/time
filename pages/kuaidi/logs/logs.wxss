.container{
	padding-bottom: 50px;
	max-height: 92%;
}
.log {
	color: #999;
	margin: 15px 10px;
}
.log_item{
	padding: 15px 5px;
}
.log_item text {
	padding-right: 20px;
}
.more{
	position: absolute;
	bottom: 0px;
	width: 100%;
  text-align: center;
}
.more .more_btn {
	margin: 10px;
	height: 35px;
	line-height: 35px;
	color:#fff;
	background-color: #03a9f4;
	border-radius: 2px;
  width: 80%;
}

.divide {
  width: 100%;
  height: 1rem;
  border-bottom: solid 1px #eee;
}

.dot_time {
  margin-top: 0.08rem;
  margin-bottom: 1rem;
  width: 1rem;
  height: 1rem;
  border-radius: 10rem;
}

.time_text {
  margin-left: 1rem;
}

.time_row {
  display: flex;
}
